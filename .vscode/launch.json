{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET MAUI",
            "type": "maui",
            "request": "launch",
            "preLaunchTask": "maui: Build"
        },
        {
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}/vscode/bookshelf/"
            ],
            "name": "启动扩展",
            "outFiles": [
                "${workspaceFolder}/vscode/bookshelf/out/**/*.js"
            ],
            "preLaunchTask": "webpack",
            "request": "launch",
            "type": "extensionHost"
        },
        {
            "name": "Python 调试程序: 当前文件",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Run Android",
            "cwd": "${workspaceFolder}/react/pingren",
            "type": "reactnative",
            "request": "launch",
            "platform": "android",
            "enableDebug": false,
            "isDynamic": true
        }
    ]
}